{
    "name": "Circlemarkr",
    "version": "4.9.3",
    "description": "G+のユーザーにマークを付けます",
    "homepage_url": "https://plus.google.com/b/112102646198561777507/",
    "options_page": "options.html",
    "background": {
        "scripts": [
            "config.js",
            "background.js"
        ]
    },
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png"
    },
    "content_scripts": [
        {
            "js": [
                "jquery-1.8.3.min.js",
                "config.js",
                "Circlemarkr.js"
            ],
            "matches": [
                "https://plus.google.com/*"
            ],
            "all_frames": true
        }
    ],
    "permissions": [
        "storage"
    ],
    "manifest_version": 2
}
